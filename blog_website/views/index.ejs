<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <%- include("navbar.ejs") %>
        <div class="container mt-5">
            <div class="row justify-content-center">
                <div class="col-md-6">
                    <% if(locals.data) { %>
                        <% data.forEach(i => { %>
                            <div class="card">
                                <div class="card-header">
                                    <h3 id="blogTitle">
                                        <%= i.messageTitle %>
                                    </h3>


                                </div>
                                <div class="card-body">
                                    <p id="blogMessage">This is the blog message.</p>
                                </div>
                                <%    }); %>
                                    <%      } %>
                            </div>
                </div>
            </div>
        </div>
</body>

</html>